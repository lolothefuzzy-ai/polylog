    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Windows PowerShell",
            "type": "PowerShell",
            "request": "launch",
            "sessionName": "Windows PowerShell (x64)"
        },
        

        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Main",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/launcher.py",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}