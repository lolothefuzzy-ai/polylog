{
  "name": "polylog6-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "test": "vitest",
    "test:ui": "playwright test",
    "test:visual": "playwright test tests/visual --headed",
    "test:integration": "playwright test tests/integration",
    "test:performance": "playwright test tests/performance",
    "test:all": "playwright test",
    "test:watch": "playwright test --ui",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "codegen": "openapi-typescript src/polylog6/api/openapi_schema.yaml -o src/api.generated.ts",
    "build:sidecar": "python build-sidecar.py",
    "build:sidecar:all": "python build-sidecar.py && echo 'Built for current platform'"
  },
  "dependencies": {
    "@babylonjs/core": "^8.37.0",
    "@babylonjs/loaders": "^8.37.0",
    "@babylonjs/materials": "^8.37.0",
    "@tanstack/react-query": "^5.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^1.5.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.56.0",
    "openapi-typescript": "^6.7.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^1.4.0"
  }
}
